md-toolbar.toolbar(layout='row', layout-align='start center', layout-padding='')
  .navbar-left(layout='row', layout-align='center center')
    img.logo(src="logo.png")
  md-input-container.md-icon-float(flex)
    label Search
    md-icon(md-font-set="material-icons") search
    input(ng-model='search', type='text')
  .navbar-right
    blaze-template(name="loginButtons")
    md-button(class="md-icon-button")
      md-tooltip(md-visible="demo.showTooltip") Admin Settings
      md-icon(md-font-set="material-icons") settings_applications

